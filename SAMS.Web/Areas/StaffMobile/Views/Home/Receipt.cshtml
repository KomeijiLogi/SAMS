                <div class="mui-table-view-cell" style="background:transparent;background-color:#EEEEEE;">
                    <p>
                        故障
                        @if (Model.WorkOrder.BillStatus.Equals(BillStatus.Complete) || Model.WorkOrder.BillStatus.Equals(BillStatus.Dispatch))
