<body>
    <div class="mui-content">
        <div class="mui-table-view">
            <p class="mui-text-center" style="color:red;font-size:18px;">以下各项均为必填*</p>
        </div>
        <div class="mui-table-view">
            <input type="hidden" name="orderID"  id="orderID" value="@Model.WorkOrder.Id" />
            <div class="mui-table-view-cell">
                <label>设备名称<input type="text" id="SerialName" Name="SerialName" value="@Model.WorkOrder.ProductName" placeholder="请输入设备名称" /></label>

            </div>
            <div class="mui-table-view-cell">
                <label>设备序号<input type="text" id="SerialNo" Name="SerialNo" value="@Model.WorkOrder.SerialNo" placeholder="请输入设备序号"/></label>
                
            </div>
            <div class="mui-table-view-cell">
                <label>工程师<input type="text" id="Engineer" Name="Engineer" value="@Model.WorkOrder.AssignedPersonName" placeholder="请输入工程师"  /></label>
            </div>


       </div>
       <div class="mui-table-view" style="margin-top:10px;">
            <div class="mui-table-view-cell">
                <label>客户名称<input type="text" id="CustomName" Name="CustomName" value="@Model.WorkOrder.CustomerName" placeholder="请输入客户名称" /></label>

            </div>
            <div class="mui-table-view-cell">
                <label>科室名称<input type="text" id="Office" Name="Office" value="@Model.WorkOrder.Office" placeholder="请输入科室名称" /></label>

            </div>
            <div class="mui-table-view-cell" style="display:none;">
                <label>科室电话<input type="tel" id="OfficeTel" Name="OfficeTel" value="@Model.WorkOrder.OfficeTel" placeholder="请输入科室电话" /></label>

            </div>
            <div class="mui-table-view-cell">
                <label>科室联系人<input type="text" id="OfficePerson" Name="OfficePerson" value="@Model.WorkOrder.OfficePerson" placeholder="请输入科室联系人" /></label>

            </div>
            <div class="mui-table-view-cell">
                <label>联系人手机<input type="tel" id="OfficeMobile" Name="OfficeMobile" value="@Model.WorkOrder.OfficeMobile" placeholder="请输入联系人手机" /></label>

            </div>
            <div class="mui-table-view-cell">
                <label>联系人职务<input type="tel" id="OfficePosition" Name="OfficePosition" value="@Model.WorkOrder.OfficePosition" placeholder="请输入联系人职务" /></label>

            </div>
           

            @if (Model.WorkOrder.ServiceType.Equals(ServiceType.Install))
